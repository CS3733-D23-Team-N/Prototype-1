CREATE TABLE L1Nodes
(
  nodeID CHAR(10) NOT NULL,
  xcoord INT NOT NULL,
  ycoord INT NOT NULL,
  floor CHAR(2) NOT NULL,
  building VARCHAR(20) NOT NULL,
  nodeType CHAR(4) NOT NULL,
  longName VARCHAR(70) NOT NULL,
  shortName VARCHAR(25) NOT NULL,
  PRIMARY KEY (nodeID)
);

CREATE TABLE L1Edges
(
  edgeID CHAR(21) NOT NULL,
  startNode CHAR(10) NOT NULL,
  endNode CHAR(10) NOT NULL,
  PRIMARY KEY (edgeID)
);
